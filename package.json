{
  "name": "discord-controller",
  "version": "0.2.8",
  "description": "A Discord Bot remote controller using Websockets and React",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start-server start-render",
    "start-server": "node index.js",
    "start-render": "npm run dev --prefix web",
    "setup": "npm i && npm run setup --prefix web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omi036/discord-controller.git"
  },
  "author": "Omi036",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Omi036/discord-controller/issues"
  },
  "homepage": "https://github.com/Omi036/discord-controller#readme",
  "dependencies": {
    "discord.js": "^14.7.1",
    "npm-run-all": "^4.1.5",
    "websocket": "^1.0.34",
    "ws": "^8.12.0"
  }
}
